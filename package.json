{
  "name": "git-qtk",
  "version": "1.0.0",
  "description": "A toolkit which provides you a quick and easy way to extract metadata from git-based repositories.",
  "main": "app.js",
  "directories": {
    "test": "./test"
  },
  "scripts": {
    "start": "node app.js",
    "test": "mocha --timeout 10000",
    "runtime": "node --max_old_space_size=12288 ./bin/runtime.js",
    "ctt": "node ./bin/ctt.js"
  },
  "bin": {
    "git-qtk": "./bin/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imdonix/git-qtk.git"
  },
  "keywords": [
    "git"
  ],
  "author": "imdonix",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/imdonix/git-qtk/issues"
  },
  "homepage": "https://github.com/imdonix/git-qtk#readme",
  "dependencies": {
    "cli-table": "^0.3.11",
    "nodegit": "^0.27.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "mocha": "latest",
    "unzipper": "^0.10.11"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
